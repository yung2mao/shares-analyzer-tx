<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.whitetown.post.mappers.PostMapper">
    <insert id="insertBasicSharesList" parameterType="java.util.List">
        <foreach collection="list" item="item" index="index">
            insert into t_basic (id,ts_code,name,area,industry,list_date) values
            (#{item.id},#{item.tsCode},#{item.name},#{item.area},#{item.industry},#{item.listDate});
        </foreach>
    </insert>
</mapper>